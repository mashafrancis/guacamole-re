{
  "name": "guacamole-re",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --watchAll",
    "test:unit": "jest -u --maxWorkers=4",
    "test:coverage": "yarn test:unit --coverage",
    "test:git": "CI=true react-scripts test",
    "eject": "react-scripts eject",
    "eslint": "eslint --report-unused-disable-directives --format codeframe '**/*.{js,jsx,ts,tsx,json}'",
    "eslint:fix": "yarn eslint --fix",
    "tslint": "tsc --noEmit && eslint \"*/**/*.{js,ts,tsx}\" --quiet",
    "prettier": "prettier  --single-quote --write \"*/**/*.{ts, js, tsx}\"",
    "docker": "docker build -t mobilities-docker .",
    "docker-start": "docker run -p 3000:80 mobilities-docker",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "assets": "rimraf ./public/assests && cp-cli ./node_modules/mobilities-assets/assets ./public/assets",
    "postinstall": "yarn run assets"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "aos": "^2.3.4",
    "clsx": "^1.1.1",
    "history": "^5.0.0",
    "mobilities-assets": "git+https://github.com/mashafrancis/mobilities-assets.git#1.0.0",
    "mq-polyfill": "^1.1.8",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-cookie-consent": "^6.1.0",
    "react-dom": "^17.0.1",
    "react-lazy-load-image-component": "^1.5.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.2.2",
    "@types/aos": "^3.0.3",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-lazy-load-image-component": "^1",
    "@types/react-router-dom": "^5.1.6",
    "@types/rimraf": "^3",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "babel-plugin-import": "^1.13.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "cp-cli": "^2.0.0",
    "customize-cra": "^1.0.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest-dom": "^3.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.5.1",
    "prettier": "^2.2.0",
    "prettier-eslint-cli": "^5.0.0",
    "pretty-quick": "^3.1.0",
    "react-app-rewired": "^2.1.6",
    "react-refresh": "^0.9.0",
    "react-styleguidist": "^11.1.3",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn prettier && yarn tslint"
    }
  }
}
